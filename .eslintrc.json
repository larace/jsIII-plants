{
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es2021": true
    },
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "prettier/prettier": "error"
    }
  }
